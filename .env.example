# RPC Endpoints (comma-separated)
RPC_ENDPOINTS=https://bsc-dataseed.binance.org,https://bsc-dataseed1.defibit.io,https://bsc-dataseed1.ninicoin.io,https://bsc.publicnode.com,https://bsc-rpc.publicnode.com

# WebSocket Endpoints (comma-separated)
WS_ENDPOINTS=wss://bsc-ws-node.nariox.org:443,wss://bsc.publicnode.com

# Trading Parameters
MIN_PROFIT_PERCENTAGE=0.5
MAX_SLIPPAGE=1.0
GAS_PRICE_GWEI=5

# Token Monitoring (leave empty to monitor all major tokens)
# Format: symbol1,symbol2,symbol3
MONITORED_TOKENS=

# Alerts
ALERT_WEBHOOK_URL=
DISCORD_WEBHOOK_URL=
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Logging
LOG_LEVEL=info
LOG_TO_FILE=true

# Performance
MAX_PAIRS_TO_MONITOR=30
CACHE_SIZE=1000
BLOCK_PROCESSING_TIMEOUT_MS=5000

# Node Environment
NODE_ENV=development
